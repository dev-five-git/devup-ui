  "author": "devfive",
  "license": "Apache-2.0",
  "homepage": "https://devup-ui.com",
  "bugs": {
    "url": "https://github.com/dev-five-git/devup-ui/issues",
    "email": "contact@devfive.kr"
  },
  "keywords": [
    "css",
    "css-in-js",
    "css-in-js-preprocessor",
    "css-in-js-framework",
    "react"
